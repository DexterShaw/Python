import itertools
U = {'ğŸ‘»', 'ğŸ•µ', 'ğŸ”º', 'ğŸ‰', 'ğŸ', 'ğŸ¦‚', 'ğŸ”¥', 'ğŸŒ»', 'ğŸ™', 'ğŸŒŒ'}
result = set()
s = set()
def findsubsets(s, n):
    return list(itertools.combinations(s, n))
for i in range(11):
    s = findsubsets(U, i)
    result = result.union(s)
print(result)

print(len(result))
